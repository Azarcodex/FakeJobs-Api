{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/jobs/_limit=(\\d+)",
      "dest": "/jobs?_limit=$1"
    },
    {
      "src": "/jobs/([^/]+)",
      "dest": "/jobs/:id"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
